{
  "name": "bun-api-project",
  "scripts": {
    "start": "bun dist/app.js",
    "prestart": "bun run build",
    "dev": "bun --watch src/app.ts",
    "build": "bun build src/app.ts --target=bun --outdir=dist --outfile=dist/app.js --minify --sourcemap=linked",
    "test": "bun test"
  },
  "module": "src/app.ts",
  "type": "module",
  "private": true,
  "dependencies": {
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "helmet": "^8.0.0"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.8",
    "@types/bun": "latest",
    "@types/express": "^4.17.17"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
